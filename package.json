{
  "name": "pixorapayments",
  "version": "1.0.0",
  "description": "Photobooth Payment Launcher with Cashfree UPI Integration",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "dev": "concurrently \"node server.js\" \"electron .\""
  },
  "keywords": [
    "photobooth",
    "cashfree",
    "electron",
    "photorooms"
  ],
  "author": "Pixora Photorooms",
  "license": "MIT",
  "build": {
    "appId": "com.pixora.photorooms",
    "productName": "Pixora Photorooms",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cashfree-pg": "^5.1.0",
    "dotenv": "^17.2.3",
    "electron": "^39.2.6",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12"
  }
}
